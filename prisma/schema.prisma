// prisma/schema.prisma

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Project {
  // id được tự động tăng (auto-increment) và là khóa chính
  id          Int    @id @default(autoincrement()) 
  project_key String @unique // project_key phải là duy nhất
  project_name String
}